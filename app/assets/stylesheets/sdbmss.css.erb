  position: relative;
  display: inline-block;
  vertical-align: middle;
}
.main-content {
  position: relative;
  height: calc(100vh - 50px);
  overflow-y: auto;
  overflow-x: hidden;
}
#main-container {
  padding: 0px 20px;
}
#main-container.container-fluid {
  position: absolute;
  width: 100%;
}
.main-content-wrapper {
  width: 100%;
  height: calc(100vh - 50px);
  vertical-align: top;
  display: table-cell;
}
.sidebar {
  width: 388px;
  margin-right: 0px;
  position: fixed;
  height: 100%;
  right: 0px;
  padding: 6px;
  padding-right: 0px;
  z-index: 9000;
}
.sidebar.collapse {
  margin-right: -388px;
  display: block;
}
.sidebar.collapse.in {
  margin-right: 0px;
}
.transition {
  -webkit-transition: all .5s !important;
  -o-transition: all .5s !important;
  transition: all .5s !important;
}

