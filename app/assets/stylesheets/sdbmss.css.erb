.faq .checkbox {
  display: none;
}
.faq .checkbox + label {
  background: #eee;
  line-height: 50px;
  margin: 0px; 
  color: white;
  border-radius: 4px;
  padding: 2px;
  text-align: center;
  font-weight: bold;
  display: inline-block;
  font-size: 38px;
