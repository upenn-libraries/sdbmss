html {
  height: 100%;
  width: 100%;
  min-width: 320px;
  font-size: 16px;
}
body {
  background: white;
  min-height: 100vh;
  width: 100%;
  display: table;
  table-layout: fixed;
  /*display: flex;*/
  font-family: Sarala, Ubuntu, sans-serif;
  overflow: hidden;
  padding-top: 50px;
}

textarea { resize:vertical ; }

h1, .h1, h2, .h2, h3, .h3, legend {
  /*font-family: Dosis;*/
  font-weight: normal;
}
a {
  /*color: #4e76b3; ... eventually */
}

#spinner {
  line-height: 40px;
  position: relative;
  margin: 0px 10px;
  float: left;
}

.forum-feed {
  width: 520px;
  display: inline-block;
  float: left;
  /* margin: 10px; */
  margin: 8px 40px 8px 0px;
  padding: 20px 20px 20px 70px;
  background: white;
  border: 1px solid #eee;
  border-radius: 2px;
}
.forum-feed h3 {
  margin-top: 0px;
  border-bottom: 1px solid #eee;
  margin: -5px -20px 15px -71px;
  padding-bottom: 5px;
}
.thredded--main-container {
  max-width: none;
  padding: 0;  
  font-family: Sarala, Ubuntu, sans-serif;
  font-size: inherit;
  color: inherit;
}
.thredded--button, .thredded--form--submit {
  background: #337ab7;
  border-color: #2e6da4;
}
.thredded--button:active, 
.thredded--button:focus,
.thredded--button:hover,
.thredded--form--submit:active, 
.thredded--form--submit:focus, 
.thredded--form--submit:hover {
  background: #286090;
  border-color: #204d74;
}
.thredded-nav {
  margin: -18px 0px 10px 0px;
  background: #f5f5f5;
  border: 1px solid #ddd;
  border-top: none;
  border-bottom-right-radius: 5px;
  border-bottom-left-radius: 5px;
}
.thredded-nav .col-sm-4 {
  margin-top: -18px;
}
li.active.forum-tab a, li.forum-tab a:hover, li.forum-tab a:focus, li.forum-tab a:active {
  background: #f5f5f5 !important;
}
.thredded--topic-header {
  margin-bottom: 2rem;
}
.thredded--navigation-breadcrumbs li {
  font-weight: normal;
}
.thredded--main-section {
  padding: 10px;
  margin-top: -10px;
}
.dashboard-forum-posts-count {
  position: static;
  margin-right: 0px;
  height: 1rem;
  width: 1rem;
  line-height: 1rem;
  font-size: 0.7rem;
}
.thredded--user-navigation--private-topics--unread, .thredded--user-navigation--moderation--pending-count {
  border-radius: 50%;
  font-weight: bold;
}
.thredded--navigation--search  {
  position: relative;
  float: right;
  margin-right: -15px;
}
.thredded--topics--topic {
  margin: 0px 50px 10px 50px;
}
.thredded--post {
}
.thredded--post--content {
  font-size: 14px;
}
.thredded--navigation--search input[type="search"], .thredded--navigation--search input[type="search"]:focus {
  margin: 0px 0px 0 0;
  -webkit-margin-end: 0px;
}
.thredded--navigation-breadcrumbs a, li.thredded--navigation-breadcrumbs--item-no-link {
  padding: 0.75rem 0 !important;
}
.thredded--post--user-and-topic {
  font-size: 1rem;
}

.divider {
  border-bottom: 1px dashed #aaa;
}

.alert-absolute {
  margin-bottom: 10px;
}
.alerts-absolute {
  position: fixed;
  bottom: 0px;
  right: 10px;
  opacity: 0.95;
  width: 300px;
  z-index: 3;
}
.search-widgets {
  white-space: normal;
}
