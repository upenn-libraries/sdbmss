<form action="" id="place-form" class="form-horizontal sdbmss-form" ng-controller="PlaceCtrl" data-ng-cloak>
  <% if @model && @model.id %>
    <input type="hidden" id="place_id" value="<%= @model.id %>">
  <% end %>
  <%= render partial: "fields" %>
  <div class="field form-group row">
    <div class="col-sm-12 text-center"><a href="" ng-click="save()" class="btn btn-primary">Save {{ place.id ? "SDBM_PLACE_" + place.id : "New Place"}}</a></div>
  </div>
</form>

<%= render partial: "modals" %>