<script type="text/ng-template" id="createPlace.html">
  <div class="modal-header">
    <h3 class="modal-title">
      Create New Place
      <a href="" class="btn btn-default pull-right" ng-click="goBack()"><span class="glyphicon glyphicon-circle-arrow-left"></span> Go Back</a>
    </h3>
  </div>
  <div class="modal-body form-horizontal compress-whitespace fit-screen">
    <%= render partial: "places/fields" %>
  </div>
  <div class="modal-footer">
    <div class="field form-group row">
      <div class="col-sm-12 text-right"><a href="" ng-click="save()" class="btn btn-primary">Save {{ place.id ? "SDBM_PLACE_" + place.id : "New Place"}}</a></div>
    </div>
  </div>
</script>

<%= render partial: "places/modals" %>