<% if can? :manage, User %>
    <li class=" ">
        <a href="/accounts">Manage Accounts</a>
    </li>
<% else %>
    <li class="breadcrumb">
        <a href="/community">Community</a>
    </li>
<% end %>
<li class=" <%= edit ? '' : 'active' %>">
  <a href="<%= profile_path(@model.username) %>"><%= @model %>
  </a>
</li>
<% if can? :edit, @model %>
<li class=" <%= edit ? 'active' : '' %>">
  <a href="<%= edit_account_path(@model) %>">Edit <%= @model %></a>
</li>
<% end %>